# GIL Orchestrator Config
# ORCH_TOKEN se autogenera si no existe un .env (ver scripts de arranque).
# Puedes fijarlo manualmente si quieres usar siempre el mismo.
# ORCH_ACTIONS_TOKEN se genera en un archivo separado (.orch_actions_token) y se
# usa para accesos READ-ONLY (por ejemplo GPT Actions).
ORCH_TOKEN=
ORCH_ACTIONS_TOKEN=
ORCH_REPO_ROOT=./repos
ORCH_CORS_ORIGINS=http://localhost:5173,http://127.0.0.1:5173
ORCH_RATE_LIMIT_WINDOW_SECONDS=60
ORCH_SUBPROCESS_TIMEOUT=10
DEBUG=false

# License gate (GIMO WEB)
ORCH_LICENSE_KEY=
ORCH_LICENSE_URL=https://gimo-web.vercel.app/api/license/validate
ORCH_LICENSE_PUBLIC_KEY=
ORCH_LICENSE_GRACE_DAYS=7
ORCH_LICENSE_RECHECK_HOURS=24

# Security: keep disabled in production.
# Only for local development labs when intentionally bypassing license check.
ORCH_LICENSE_ALLOW_DEBUG_BYPASS=false

# Variables para Fase 2 - GIOS Bridge
COGNITIVE_GIOS_BRIDGE_ENABLED=true
